						</span>
					</div>
				</div>
				<!-- /row -->
			</div>
			<!-- /container -->
		</div>
		<!-- /bottom footer -->
	</footer>
	<script src="~/templateClient/js/jquery.min.js"></script>
	<script src="~/templateClient/js/bootstrap.min.js"></script>
    <script src="~/templateClient/js/slick.min.js"></script>
    <script src="~/templateClient/js/nouislider.min.js"></script>
	<script src="~/templateClient/js/jquery.zoom.min.js"></script>
	<script src="~/templateClient/js/main.js"></script>
	<script src="https://code.jquery.com/ui/1.13.1/jquery-ui.js"></script>

	<script src="~/templateselect/js/jquery.min.js"></script>
	<script src="~/templateselect/js/popper.js"></script>
	<script src="https://cdnjs.cloudflare.com/ajax/libs/select2/4.0.4/js/select2.min.js"></script>
	<script src="~/templateselect/js/main.js"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>